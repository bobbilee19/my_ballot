<div class="col-1-3"> <!-- Col 1 -->

	<section class="contentBox"><!-- Box -->

		<h2>Federal</h2>
		<h3><%= Position.find_by_name("President").name %> / Vice President</h3>
		<ul class="candidates">
		<% @candidatelist.each do |candidate| %>
			<% if "2012-11-06" == candidate["electionDate"] %>
			<% if "President" == candidate["electionOffice"] %>
			<li>
			<%= link_to "#{candidate['ballotName']}", candidatebio_url(:candidate_id => "#{candidate['candidateId']}") %>
			<% if candidate["runningMateName"].present? %> / 
			<%= candidate["runningMateName"] %> <span class="party"><%= candidate["electionParties"] %></span>
			<% end %>
			</li>
			<% end %>
			<% end %>
			<% end %>
		</ul>



		<h3><%= Position.find_by_name("U.S. House").name %></h3>
		<ul class="candidates">
			<% @candidatelist.each do |candidate| %>
				<% if "2012-11-06" == candidate["electionDate"] %>
				<% if "U.S. House" == candidate["electionOffice"] %>
				<li>
				<%= link_to "#{candidate['ballotName']}", candidatebio_url(:candidate_id => "#{candidate['candidateId']}") %><span class="party"><span class="district">Congressional District <%= candidate["electionDistrictName"] %>, </span><%= candidate["electionParties"] %></span> 
				</li>
				<% end %>
				<% end %>
				<% end %>
			</ul>

	</section> <!-- close Box -->
	
	<section class="contentBox"> <!-- Open box -->
			
		<h2>State</h2>

		<h3><%= Position.find_by_name("State Senate").name %></h3>
			<ul class="candidates">
			   <% @candidatelist.each do |candidate| %>
			   <% if "2012-11-06" == candidate["electionDate"] %>
			   <% if "State Senate" == candidate["electionOffice"] %>
			   <li>
			   <%= link_to "#{candidate['ballotName']}", candidatebio_url(:candidate_id => "#{candidate['candidateId']}") %><span class="party"><span class="district">State Senate District <%= candidate["electionDistrictName"] %>, </span><%= candidate["electionParties"] %></span> 
			   </li>
			   <% end %>
			   <% end %>
			   <% end %>
			</ul>
			
		<h3><%= Position.find_by_name("State House").name %></h3>
			<ul class="candidates">
				  <% @candidatelist.each do |candidate| %>
				  <% if "2012-11-06" == candidate["electionDate"] %>
				  <% if "State House" == candidate["electionOffice"] %>
				  <li>
				  <%= link_to "#{candidate['ballotName']}", candidatebio_url(:candidate_id => "#{candidate['candidateId']}") %><span class="party"><span class="district">State House District <%= candidate["electionDistrictName"] %>, </span><%= candidate["electionParties"] %></span> 
				  </li>
				  <% end %>
				  <% end %>
				  <% end %>
			</ul>

	</section> <!-- Close Box -->

	<section class="contentBox"> <!-- Open Box -->
		
		<h2>County</h2>
		<h3><%= Position.find_by_name("Metropolitan Water Reclamation District Commissioners").name %></h3>
		<h4><%= Position.find_by_name("Metropolitan Water Reclamation District Commissioners").note %></h4>
			<ul class="multi">
				<li>Debra Shore <span class="party">Democratic</span></li>
				<li>Kari K. Steele <span class="party">Democratic</span></li>
				<li>Patrick Daley Thompson <span class="party">Democratic</span></li>
				<li>Harold "Noonie" Ward <span class="party">Republican</span></li>
				<li>Carl Segvich <span class="party">Republican</span></li>
				<li>Dave Ehrlich <span class="party">Green</span></li>
				<li>Karen Roothaan <span class="party">Green</span></li>
				<li>Nasrin R. Khalili <span class="party">Green</span></li>
			</ul>
		

		<h3><%= Position.find_by_name("Clerk of the Circuit Court").name %></h3>
		<ul class="candidates">
			<li>Dorothy A. Brown <span class="party">Democratic</span></li>
			<li>Diane S. Shapiro <span class="party">Republican</span></li>
		</ul>


		<h3><%= Position.find_by_name("Recorder").name %></h3>
		<ul class="candidates">
			<li>Karen A. Yarbrough <span class="party">Democratic</span></li>
			<li>Sherri Griffith <span class="party">Republican</span></li>
		</ul>


		 	<h3><%= Position.find_by_name("State's Attorney").name %></h3>
